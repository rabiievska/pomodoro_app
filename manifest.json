{
  "manifest_version": 2,
  "name": "Pomodoro App",
  "version": "1.0.0",
  "description": "A Pomodoro extension",
  "icons": {
    "128": "./assets/images/tomato.png",
    "48": "./assets/images/tomato.png",
    "16": "./assets/images/tomato.png"
  },
  "permissions": [
    "background",
    "alarms",
    "notifications"
  ],
  "app": {
    "background": {
      "scripts": ["./dist/background.js"]
    }
  }
}
